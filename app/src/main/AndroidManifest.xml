<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.schoolmanagment">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactForm" />
        <activity android:name=".quiz" />
        <activity android:name=".aboutus" />
        <activity android:name=".TakeAttendenceList" />
        <activity android:name=".SelectingClass" />

        <activity android:name=".ShowStudent" />
        <activity android:name=".StudentListShowDelete" />
        <activity android:name=".adminpanel" />
        <activity android:name=".meeting" />
        <activity android:name=".AddStudent" />
        <activity android:name=".TeacherPower" />
        <activity android:name=".WebView"
            android:configChanges="screenSize|orientation"/>
        <activity
            android:name=".Teachercode"
            android:noHistory="true" />
        <activity android:name=".TeacherHomeScreen" />
        <activity
            android:name=".TeacherLogin"
            android:noHistory="true" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Notes" />
        <activity android:name=".subjectlist" />
        <activity android:name=".HomeScreen" />
        <activity
            android:name=".StudentLogin"
            android:noHistory="true" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service android:name=".MyMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>